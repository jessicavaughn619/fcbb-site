{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'fluid-container',
  ]
%}

{{ attach_library("fcbb_theme/featured-events") }}

<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2 class='fixed-container-xl center' {{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content.field_view }}
    <div class='swiper-navigation'>
      <div class='swiper-button-prev'></div>
      <div class='swiper-button-next'></div>
    </div>
  {% endblock %}
</div>